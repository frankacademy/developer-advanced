<Module	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="../FrankConfig.xsd" >

    <Adapter name="processfile">
        <Receiver name="processfile">
            <DirectoryListener 
                inputFolder="c:\temp\input" 
                inProcessFolder="c:\temp\inprocess" 
                processedFolder="c:\temp\processed" 
                createFolders="true"
				messageType="CONTENTS"
				fileTimeSensitive="true"
               />
        </Receiver>

		<Pipeline>
			<Exits>		
				<Exit name="SUCCESS" state="SUCCESS" />			
			</Exits>
			<CsvParserPipe name="parse CSV" fieldSeparator=";" prettyPrint="true"/>
			<ForEachChildElementPipe name="process row"
			
			>
		
		</ForEachChildElementPipe>

		</Pipeline>
	</Adapter>

</Module>